#!/usr/bin/env ruby

require_relative "../lib/user.rb"
require_relative "../lib/teacher.rb"
require_relative "../lib/student.rb"

Jim = Student.new
Jim.first_name = "Jim"
Jim.last_name = "Jeffreys" 


Avi = Teacher.new
Avi.first_name = "Avi"
Avi.last_name = "Neilson"
knowledge = Avi.teach 

Jim.learn(knowledge) 

puts "Jim just learned #{knowledge}" 